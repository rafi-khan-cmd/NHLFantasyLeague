# Environment Variables Template
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to version control

# Snowflake Configuration (Optional - app works in mock mode without this)
SNOWFLAKE_URL=jdbc:snowflake://YOUR_ACCOUNT.snowflakecomputing.com:443/?warehouse=COMPUTE_WH&db=SUPPLYCHAIN&schema=RAW
SNOWFLAKE_USERNAME=YOUR_USERNAME
SNOWFLAKE_PASSWORD=YOUR_PASSWORD
SNOWFLAKE_WAREHOUSE=COMPUTE_WH
SNOWFLAKE_DATABASE=SUPPLYCHAIN
SNOWFLAKE_SCHEMA=RAW
SNOWFLAKE_AUTO_SETUP=true

# Databricks Configuration (Optional - app uses mock mode without this)
DATABRICKS_WORKSPACE_URL=
DATABRICKS_ACCESS_TOKEN=
DATABRICKS_CLUSTER_ID=

# Database Configuration (for production)
DB_USERNAME=postgres
DB_PASSWORD=your_db_password

# RabbitMQ Configuration
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USERNAME=guest
RABBITMQ_PASSWORD=guest

# Kafka Configuration
KAFKA_BOOTSTRAP_SERVERS=localhost:9092
